{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iis": {
      "applicationUrl": "http://localhost/PastryShopApi",
      "sslPort": 0
    },
    "iisExpress": {
      "applicationUrl": "http://localhost:25557",
      "sslPort": 0
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "PastryShopApi": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ConnectionString": "Data Source=(localdb)\\MSSQLLocalDB;Database=PastryShop;Trusted_Connection=True;MultipleActiveResultSets=True",
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "ancmHostingModel": "OutOfProcess",
      "dotnetRunMessages": "true",
      "applicationUrl": "http://localhost:5000"
    }
  }
}